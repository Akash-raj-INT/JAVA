public class SumOfNaturalNumbers {
    public static void main(String[] args) {
        int n = 10; // You can change this to any positive integer
        int sum = 0;

        for (int i = 1; i <= n; i++) {
            sum += i;
        }

        System.out.println("Sum of natural numbers up to " + n + " is: " + sum);
    }
}

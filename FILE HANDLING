import java.io.File;
public class createfileZW {
 public static void main(String[] args) {
 try{
 File obj=new File("E:/LPU/Javacode/javacodes/k22ZW.txt");
 if(obj.createNewFile())
 {
 System.out.println("file Created ");
 }
 else{
 int counter=1;
 while(true){
 File objj=new File("E:/LPU/Javacode/javacodes/k22ZW"+counter+".txt");
 if(objj.createNewFile()){
 System.out.println("File Created ");
 break;
 }else{
 System.out.println("File NOte Creayed ");
 }
 counter++;
 }
 }
 }
 catch(Exception e){

 }
 }
 
}
